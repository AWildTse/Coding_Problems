public class Solution {
    public int[] RunningSum(int[] nums) {
        int currTotal = 0;
        int count = 0;
        int[] newArr = new int[nums.Length];
        for(int i = 0; i < nums.Length; i++)
        {
            currTotal += nums[i];
            newArr[i] = currTotal;
        }
        return newArr;
    }
}
